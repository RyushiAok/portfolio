{
  "name": "client",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"pnpm run start\"",
    "lint": "pnpm biome lint ./app",
    "format": "biome format --write ./app",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "deploy": "pnpm run build && wrangler pages deploy ./public"
  },
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.3",
    "@remix-run/cloudflare": "^2.7.2",
    "@remix-run/cloudflare-pages": "^2.7.2",
    "@remix-run/css-bundle": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.7.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "typescript": "^5.1.0",
    "wrangler": "^3.24.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
